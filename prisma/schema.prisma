generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id        String @id @db.VarChar(36)
  login     String @db.VarChar(30)
  password  String @db.VarChar(30)
  version   Int
  createdAt Int
  updatedAt Int
}

model artists {
  id     String  @id @db.VarChar(36)
  name   String  @db.VarChar(30)
  grammy Boolean
}
